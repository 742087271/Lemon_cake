
page{
    box-sizing: border-box;
    padding:30rpx;
    width: 100%;
}
.add-img-list{
    padding-top:20rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}
.added-img{
    border:1rpx solid rgb(196, 194, 194);
    border-radius: 20rpx;
    width:200rpx;
    height: 200rpx;
}
.add-img-list-item{
    box-sizing: content-box;
    margin: 10rpx 0rpx;
    width: 33.33%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.my-input{
    width: auto;
    border:1rpx solid #ddd;
    border-radius: 10rpx;
    padding:5rpx 10rpx;
    font-size:24rpx;

}
.add-message-view .title{
    line-height: 70rpx;
    height: 50rpx;
    font-size: 30rpx;
    font-weight: bold;
    padding:20rpx 0rpx;
}
.align-between{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
