/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.ell{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.divider{
  background: #eee;
  height:15rpx;
}
.cell{
    background:#fff;
    font-size:16px;
}
.cell-item{
    padding:20rpx 30rpx;
    border-bottom:1px solid #ddd;
}
.arrow{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.arrow::after{
    content: "";
    width: 15rpx;
    height:15rpx;
    border-top:3rpx solid #ccc;
    border-right:3rpx solid #ccc;
    transform:rotate(45deg);
}