

<view class="add-img-list">
    <view class=" add-img-list-item" wx:for="{{imgList}}" wx:key="index" >
        <image class="added-img" src="{{item.url}}" mode="widthFix"/>
    </view>
    <view class=" add-img-list-item">
        <view class="wx_UploadImg_container">
            <view class="draw-mask" bindtap="selectImg">
                <button  class="btn btn-asc"></button>
            </view>
        </view>
    </view>   
</view>   

